{
    "name": "yiisoft/inflector",
    "type": "library",
    "description": "Yii - Inflector Helper",
    "keywords": [
        "yii",
        "helper",
        "inflector"
    ],
    "homepage": "http://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Alexander Makarov",
            "email": "sam@rmcreative.ru",
            "homepage": "http://rmcreative.ru/",
            "role": "Core framework development"
        },
        {
            "name": "Antonio Ramirez",
            "email": "amigo.cobos@gmail.com",
            "homepage": "http://www.2amigos.us/",
            "role": "Core framework development"
        }
    ],
    "support": {
        "source": "https://github.com/yiisoft/inflector",
        "issues": "https://github.com/yiisoft/inflector/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2",
        "yiisoft/strings": "^3.0@dev",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2"
    },
    "suggest": {
        "ext-intl": "PHP Internationalization extension"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Helpers\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Helpers\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
